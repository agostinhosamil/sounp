{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./src/*"
      ],
      "@styles": [
        "./src/styles"
      ],
      "@images/*": [
        "./src/images/*"
      ],
      "@images": [
        "./src/images"
      ],
      "@components": [
        "./src/components"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@reducers/*": [
        "./src/config/state/reducers/*"
      ]
    }
  }
}
